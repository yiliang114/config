{
  "name": "eslint-config-wire",
  "version": "0.0.1",
  "scripts": {
    "lint": "vue-cli-service lint",
    "fix": "eslint --fix \"**/*.{js,jsx,vue}\""
  },
  "main": "index.js",
  "dependencies": {
    "eslint-config-airbnb-base": "^13.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^4.4.6",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^1.3.0",
    "lint-staged": "^8.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yiliang114/eslint-config-wire.git"
  }
}
